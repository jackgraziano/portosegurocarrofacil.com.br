	<script type="text/javascript">
		function leTrim(x) {
			console.log(x);
			if(x){
				return x.replace(/^\s+|\s+$/gm,'');
			} else {
				return "";
			}
		}

		function sendFormContact(){
			var check = verifyFormReturn('formSendContato');
			var CPF_Contact = document.formSendContato.CPF_Contact.value;
			if(check > 0){
				openModalBox("ErrorModal", 'OPS! Ocorrreu um erro|Preencha os campos corretamente|OK');
			} else {
				$("#formSendContato").css("opacity",0.2);
				$.ajax({
					type: "POST",
					url: rootSite + "module/porto-seguro-corretor/",
					data: {
						"action" : "buscaCorretorCPF",
						"cpf" : CPF_Contact
					},
					accepts: {xml: "text/xml", text: "text/plain"},
					dataType: 'xml',
					success: function (xml) {

					// console.log(xml);
					status = $(xml).find("statusSegurado").text();
					susep = $(xml).find("susep").text();
					if(status > 0){
						document.formSendContato.Corretor_Contact.value = leTrim(status) + "|||" + leTrim(susep);
					}
					document.formSendContato.submit();
					//$("#formSendContato").css("opacity",1);
				}
			});
			}
			return false;
		}
	</script>
