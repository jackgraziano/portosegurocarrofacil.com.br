<script>
	var navCotacao = 1;

	$('.step').scroll(function(event) {
		if ($('.step').scrollLeft() < 10) {
			$('.bg-inicio-bullet').hide();
		}else {
			$('.bg-inicio-bullet').show();
		}

		if ($('.step').scrollLeft() > 190) {
			$('.bg-fim-bullet').hide();
		}else {
			$('.bg-fim-bullet').show();
		}
	});

	$(document).ready(function() {
		switch(navCotacao) {
			case 1:
			$('.step').scrollLeft( 0 );
			$('.bg-inicio-bullet').hide();
			break;

			case 2:
			$('.step').scrollLeft( 80 );
			break;

			case 3:
			$('.step').scrollLeft( 160 );
			break;

			case 4:
			$('.step').scrollLeft( 240 );
			break;

			case 5:
			$('.step').scrollLeft( 320 );
			break;

			case 6:
			$('.step').scrollLeft( 400 );
			$('.bg-fim-bullet').hide();
			break;
		}
	});	
</script>

